<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminDashboard">
    <TextView
        android:id="@+id/text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="NIRVOR DASH BOARD"
        android:textSize="30sp"
        android:layout_marginTop="40dp"
        android:textAlignment="center"


        />

    <Button
        android:id="@+id/bt3"
        android:layout_width="100dp"
        android:layout_height="100dp"

        android:layout_gravity="right"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="100dp"
        android:text="USER"
        android:textColor="#FEFEFE"
        android:background="@drawable/button_background2"
        android:onClick="goUser"
        />

    <Button
        android:id="@+id/bt4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_toRightOf="@+id/bt3"
        android:layout_gravity="right"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="100dp"
        android:background="@drawable/button_background2"
        android:text="APPLIED USER"
        android:textColor="#FEFEFE"
        android:onClick="goNewUser"

        />

    <Button

        android:id="@+id/bt5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_below="@+id/bt3"
        android:layout_gravity="right"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button_background2"
        android:text="BRANCH GROWTH AND REALIZATION"
        android:textColor="#FEFEFE"
        android:onClick="branchGrowthGo"

        />


    <Button
        android:id="@+id/bt7"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_toRightOf="@+id/bt5"
        android:layout_below="@+id/bt4"
        android:layout_gravity="right"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button_background2"
        android:text="PREDICT INSPECTION"
        android:textColor="#FEFEFE"
        android:onClick="predictInspectionGo"
        />


    <Button

        android:id="@+id/bt6"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_below="@+id/bt5"
        android:layout_gravity="right"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button_background2"
        android:text="PO EVALUATION"
        android:textColor="#FEFEFE"
        android:onClick="poEvaluateGo"

        />

    <Button
        android:id="@+id/bt8"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_toRightOf="@+id/bt6"
        android:layout_below="@+id/bt7"
        android:layout_gravity="right"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button_background2"
        android:text="Question Query"
        android:textColor="#FEFEFE"
        android:onClick="query"
        />


    <Button
        android:layout_centerHorizontal="true"
        android:id="@+id/map"
        android:layout_width="100dp"
        android:layout_height="100dp"

        android:layout_below="@+id/bt8"
        android:layout_gravity="right"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/button_background2"
        android:text="Vo Location"
        android:textColor="#FEFEFE"
        android:onClick="goMap"
        />

</RelativeLayout>